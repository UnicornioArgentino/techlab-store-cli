    "description":  "CLI en Node.js para gestionar productos usando FakeStoreAPI (GET/POST/DELETE)",
    "type":  "module",
    "main":  "index.js",
    "scripts":  {
                    "start":  "node index.js",
                    "get:all":  "node index.js GET products",
                    "get:one":  "node index.js GET products/5",
