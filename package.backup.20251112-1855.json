                    "del:demo":  "node index.js DELETE products/7",
                    "get:cat:list":  "node index.js GET products/category/list",
                    "get:cat:elec":  "node index.js GET products/category/electronics",
                    "export:all":  "node index.js EXPORT products",
                    "filter:1":  "node index.js GET \u0027products?minPrice=100\u0026minRating=4\u0027",
                    "filter:2":  "node index.js GET \u0027products?category=electronics\u0026maxPrice=200\u0027",
